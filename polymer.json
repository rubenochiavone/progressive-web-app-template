{
  "entrypoint": "index.html",
  "shell": "src/progressive-app.html",
  "fragments": [
    "src/app-lazy-resources.html",
    "src/app-pages/app-page-404/app-page-404.html",
    "src/app-pages/app-page-contact-us/app-page-contact-us.html",
    "src/app-pages/app-page-home/app-page-home.html",
    "src/app-pages/app-page-sign-up/app-page-sign-up.html",
    "src/app-pages/app-pages-sign-in/app-page-enter-your-email/app-page-enter-your-email.html",
    "src/app-pages/app-pages-sign-in/app-page-forgot-password/app-page-forgot-password.html",
    "src/app-pages/app-pages-sign-in/app-page-sign-in/app-page-sign-in.html",
    "src/app-pages/app-pages-sign-in/app-page-sign-in-with-email/app-page-sign-in-with-email.html",
    "src/app-pages/app-pages-sign-in/app-pages-sign-in.html",
    "src/app-service-worker.html",
    "src/app-shell/app-shell-drawer.html",
    "src/app-shell/app-shell-lazy-resources.html",
    "src/app-ui-components/app-snackbar/app-snackbar.html"
  ],
"sources": [
    "src/**/*",
    "images/**/*",
    "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "name": "es5-bundled",
    "bundle": true,
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "addPushManifest": true
  },{
    "name": "es6-unbundled",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "addPushManifest": true
  }]
}
